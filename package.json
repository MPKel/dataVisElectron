{
  "name": "SOTD_Visualization",
  "version": "0.0.2",
  "description": "An app to determine effectiveness of SOTD emails",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "npm install && electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "SOTD"
  ],
  "build": {
    "appId": "SOTD_VisualizationMPK",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "author": "Michael Kelley",
  "license": "MIT",
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "latest"
  },
  "dependencies": {
    "ajv": "^6.5.5",
    "d3": "^5.7.0",
    "jquery": "^3.3.1"
  }
}
